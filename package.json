{
  "name": "magda-web",
  "version": "0.0.1",
  "private": true,
  "homepage": ".",
  "config": {
    "awsProfile": "terria",
    "awsS3BasePath": "s3://magda-dev.terria.io",
    "awsS3Dir": "$(git rev-parse --abbrev-ref HEAD)"
  },
  "devDependencies": {
    "deep-freeze": "0.0.1",
    "expect": "^1.20.2",
    "nock": "^9.0.2",
    "react-scripts": "0.7.0",
    "redux-mock-store": "^1.2.1"
  },
  "dependencies": {
    "d3-drag": "^1.0.1",
    "d3-selection": "^1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^0.7.7",
    "leaflet-mapbox-vector-tile": "TerriaJS/Leaflet.MapboxVectorTile",
    "lodash.debounce": "^4.0.8",
    "lodash.find": "^4.6.0",
    "lodash.findindex": "^4.6.0",
    "lodash.isequal": "^4.4.0",
    "lodash.max": "^4.0.1",
    "lodash.maxby": "^4.6.0",
    "lodash.min": "^4.0.1",
    "lodash.minby": "^4.6.0",
    "marked": "^0.3.6",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.6",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "rm -rf node_modules && npm install && npm run deploy-without-reinstall",
    "deploy-without-reinstall": "npm run build && npm run deploy-current",
    "deploy-current": "bash -c \"aws --profile $npm_package_config_awsProfile s3 sync --delete ./build/ $npm_package_config_awsS3BasePath/$(eval echo $npm_package_config_awsS3Dir)\""
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
